{
  "name": "i-earvin",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "npm run format:check && next build",
    "build:local": "npm run format && next build",
    "start": "next start",
    "format": "NODE_OPTIONS='--experimental-strip-types' prettier --write .",
    "format:check": "NODE_OPTIONS='--experimental-strip-types' prettier --check .",
    "lint": "eslint .",
    "prisma:push": "dotenv -e .env.local -- npx prisma db push",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "prepare": "husky",
    "cosmos": "cosmos --expose-imports",
    "cosmos-export": "cosmos-export --expose-imports"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^16.0.0",
    "@contentful/rich-text-types": "^17.0.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "6.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tiptap/react": "^2.10.4",
    "@tiptap/starter-kit": "^2.10.4",
    "accept-language": "^3.0.20",
    "contentful": "^11.2.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.468.0",
    "next": "15.4.5",
    "postcss": "^8.5.6",
    "prisma": "^6.2.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.5.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.4.0-canary.51",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-cosmos": "^7.0.0",
    "react-cosmos-next": "^7.0.0",
    "react-cosmos-plugin-open-fixture": "^7.0.0",
    "sass": "^1.84.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
